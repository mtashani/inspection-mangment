{
  "name": "frontend-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:turbo": "next dev -p 3001 --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "build:docker": "docker build -t inspection-frontend-v2 .",
    "start": "next start -p 3001",
    "start:prod": "NODE_ENV=production next start -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "dev:clean": "npm run clean && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "docker:build": "docker build -t inspection-frontend-v2 .",
    "docker:run": "docker run -p 3001:3001 inspection-frontend-v2",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "check-all": "npm run type-check && npm run lint && npm run format:check && npm run test:ci"
  },
